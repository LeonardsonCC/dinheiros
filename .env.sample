# Database type: "sqlite3" or "postgres"
DB_TYPE=sqlite3
# SQLite database path
DB_PATH=./dinheiros.db
# PostgreSQL settings (used if DB_TYPE=postgres)
DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASS=password
DB_NAME=dinheiros
# Server port
PORT=8080
# JWT Configuration
JWT_SECRET_KEY=your-super-secret-jwt-key-change-this-in-production
JWT_TOKEN_DURATION_HOURS=168
